{
    "manifest_version": 2,
    "name": "referer-fixer",
    "description": "Change the referer header for any domain",
    "version": "0.0.1",
    "options_ui": {
	"page": "options.html"
    },
    "permissions": [
	"activeTab",
	"storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
	"page": "background.html"
    },
    "icons": {
	"128": "icon128.png"
    }
}
